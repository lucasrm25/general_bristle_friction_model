component damper2d < generalBristleFriction2d.branch2d

annotations
%     N : ExternalAccess=modify
    Icon = "images/damper2d.svg"
end

parameters
    dmp_rate = { 10*eye(2), 'N*s/m' };  % Spring rate
end

variables
    x = { value = { [0;0], 'm'}, priority = priority.high };    % Deformation x
end

equations
    v == x.der;
    f == dmp_rate * v;
end

end